<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.rbt.primetest.controller.IndexBean" table="BRM_ORDER">
        <id name="ID_PK" type="int">
            <column name="USER_ID" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="DATE_OF" type="string">
            <column name="USERNAME" length="20" not-null="true" />
        </property>
        <property name="CUSTOMER_ID" type="int">
            <column name="CREATED_BY" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>